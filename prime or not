import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner digit=new Scanner(System.in);
	    int num=digit.nextInt();
	    int count=0;
	    for(int i=1;i<=num;i++)
	 {
	     if(num%i==0){
	         count++;
	     }
	 }
	     System.out.println(count==2?("PRIME NUMBER"):("NOT A PRIME NUMBER"));
	     
	
}
}
